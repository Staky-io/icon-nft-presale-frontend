{
    "vetur.validation.template": false,
    "eslint.options": {
        "extensions": [
            ".html",
            ".js",
            ".jsx",
            ".vue"
        ]
    },
    "eslint.packageManager": "npm",
    "eslint.validate": [
        "html",
        "vue",
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "todo-tree.tree.showScanModeButton": false,
    "eslint.codeAction.disableRuleComment": {}
}
